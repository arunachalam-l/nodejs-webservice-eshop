<?xml version="1.0" encoding="UTF-8"?>
<suite name="NodejsTestSuite" parallel="tests" thread-count="10"
	verbose="3">
	<test name="VerifyWelcomePageOnGooglechrome" junit="false"
		preserve-order="true">
		<parameter name="browser" value="googlechrome" />
		<parameter name="platform" value="WINDOWS" />
		<classes>
			<class name="com.photon.phresco.nodejs.testcases.NodejsTestCase" />
		</classes>
	</test>
	 <test name="VerifyWelcomePageOnFirefox" junit="false"
		preserve-order="true">
		<parameter name="browser" value="firefox" />
		<parameter name="platform" value="WINDOWS" />
		<classes>
			<class name="com.photon.phresco.nodejs.testcases.NodejsTestCase" />
		</classes>
	</test> 
		 <test name="VerifyWelcomePageOnHTMLUnit" junit="false"
		preserve-order="true">
		<parameter name="browser" value="htmlunit" />
		<parameter name="platform" value="WINDOWS" />
		<classes>
			<class name="com.photon.phresco.nodejs.testcases.NodejsTestCase" />
		</classes>
	</test> 
</suite>

 
